<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="line">
            <size android:width="@dimen/dp_60" android:height="@dimen/dp_3" />
            <solid android:color="@color/color_09bb07" />
            <stroke android:width="@dimen/dp_2" android:color="@color/color_09bb07" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="line">
            <size android:width="@dimen/dp_60" android:height="@dimen/dp_3" />
            <solid android:color="@color/color_09bb07" />
            <stroke android:width="@dimen/dp_2" android:color="@color/color_09bb07" />
        </shape>
    </item>
    <item>
        <shape android:shape="line">
            <size android:width="@dimen/dp_60" android:height="@dimen/dp_3" />
            <stroke android:width="@dimen/dp_2" android:color="@color/color_eeeeee" android:dashGap="@dimen/dp_5" android:dashWidth="@dimen/dp_2" />
        </shape>
    </item>
</selector>